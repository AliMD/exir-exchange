appId='exir-exchange'
deployHost='mci2'
deployPath="/srv/${appId}"

appDomains='exex.exchange,www.exex.exchange'
appForceDomain='www.exex.exchange'

appImage='ghcr.io/exirworld/exir-exchange-app:next'
